<p id="notice"><%= notice %></p>

<h1 id="images_heading">Images</h1>

<table>
  <thead>
    <tr>
      <th>Url name</th>
      <th>Title</th>
      <th>Caption</th>
      <th>Price</th>
      <th>Human type</th>
      <th>Popular</th>
      <th>Arrival</th>
      <th>Saved to list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.url_name %></td>
        <td><%= image.title %></td>
        <td><%= image.caption %></td>
        <td><%= image.price %></td>
        <td><%= image.human_type %></td>
        <td><%= image.popular %></td>
        <td><%= image.arrival %></td>
        <td><%= image.saved_to_list %></td>
        <td><%= link_to 'Show', image %></td>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image', new_image_path %>
